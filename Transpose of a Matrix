import numpy as np


def transpose(arr):
    return np.transpose(np.array(arr, dtype=object)).tolist()


print(transpose([['d', 'a', 11, 9, 5, 15, 3, 11], [11, 'f', 'a', 'd', 13, 'e', 1, 20]]))


